   "source": [
    "def deco(func):\n",
    "    def wrapper(arg):\n",
    "        print('----- start -----')\n",
    "        print(func(arg))\n",
    "        print('----- end -----')\n",
    "    return wrapper\n",
    "\n",
    "\n",
    "def hello(str):\n",
    "    return str \n",
    "\n",
    "decorated_hello = deco(hello)\n",
    "decorated_hello(\"Hello World!\")\n"
   ]
